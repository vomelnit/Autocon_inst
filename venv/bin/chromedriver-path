#!/home/vomel/PycharmProjects/Autocon_instagram_poster/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'chromedriver-binary==2.42.0','console_scripts','chromedriver-path'
__requires__ = 'chromedriver-binary==2.42.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('chromedriver-binary==2.42.0', 'console_scripts', 'chromedriver-path')()
    )
